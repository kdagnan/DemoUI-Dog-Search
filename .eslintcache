\n    \n    //Filter for dogs matching text search box\n    matchingDogs.push(...dogs.filter(d => \n        (d.name.toLowerCase().includes(search) || \n        d.description.toLowerCase().includes(search)|| \n        d.owner.toLowerCase().includes(search)) \n        ) );\n    \n    //remove those not matching breed/size req\n    matchingDogs = matchingDogs.filter(d => (\n        (d.breed === breedFilter || breedFilter == '') && \n        (d.size === sizeFilter || sizeFilter === '')\n    ))\n\n    //Ordering\n    if(orderBy === 'size'){\n        matchingDogs.sort((a,b) => getSizeNum(a.size) > getSizeNum(b.size) ? 1 : -1 );\n    }else{\n        matchingDogs.sort((a,b) => a[orderBy] > b[orderBy] ? 1 : -1);\n    }\n\n    let mapDogList = () => {\n        if(matchingDogs.length > 0){\n            return matchingDogs.map((d) => {\n                return <DogCard key={d.id} dog={d} setRefresh={setRefresh}/>\n            })\n        }else{\n            return <Message message={'No Matching Dogs Found'} iconClass={'fa-exclamation-circle'}/>\n        }\n    }\n\n    return (\n        <div>\n            <EditModal update={setRefresh}/>\n            <div className=\"form-group form-inline w-25\">\n                <label htmlFor=\"exampleFormControlSelect1\">Order By</label>\n                <select className=\"form-control ml-0\" id=\"exampleFormControlSelect1\" style={{width: '150px' }} value={orderBy} onChange={(e) => setOrderBy(e.target.value)}>\n                    <option value='name'>Name</option>\n                    <option value='owner'>Owner</option>\n                    <option disabled={sizeFilter.length > 0} value='size'>Size</option>\n                    <option disabled={breedFilter.length > 0} value='breed'>Breed</option>\n                </select>\n            </div>\n            <div className='card bg-light'>\n                <div className='card-body'>\n                    { mapDogList() }\n                    { matchingDogs.length > 0 ? <p className='pull-right'>Showing {matchingDogs.length} matching dogs.</p> : null }\n                </div>\n            </div>\n        </div>\n    )\n}\n","/Users/kyled/Desktop/voyant-challenge/src/InitialDogData.js",["48"],"export default [\n    {\n      id: 1,\n      name: \"Saul\",\n      breed: \"Lab\",\n      owner: \"Dave\",\n      size: \"LG\",\n      description: \"An energetic lab that loves to play\",\n      image: \"https://www.harlingenveterinaryclinic.com/sites/default/files/styles/large/adaptive-image/public/labrador-retriever-dog-breed-info.jpg?itok=GAXLXkT7\"\n    }, \n    {\n      id: 2,\n      name: \"Goldie\",\n      breed: \"Lab\",\n      owner: \"Dave\",\n      size: \"LG\",\n      description: \"Office mommy dog\",\n      image: \"https://i0.wp.com/puppytoob.com/wp-content/uploads/2018/11/english-labrador-life-span.jpg?w=650&ssl=1\"\n    }, \n    {\n      id: 3,\n      name: \"Gio\",\n      breed: \"Terrier\",\n      owner: \"Dan\",\n      size: \"SM\",\n      description: \"A fluffy terrier mix who runs on treats\",\n      image: 'https://bowwowinsurance.com.au/wp-content/uploads/2018/10/border-terrier-700x700.jpg'\n    }, \n    {\n      id: 4,\n      name: \"Sophie\",\n      breed: \"Pit Bull\",\n      owner: \"Christina\",\n      size: \"MD\",\n      description: \"A brindled pitbull princess\",\n      image: 'https://figopetinsurance.com/sites/default/files/styles/blog_detail/public/imagedogsrescued-tan-pit-bull-laying-groundblog.jpg?itok=zddeTs1C'\n    }, \n    {\n      id: 5,\n      name: \"Taro\",\n      breed: \"Great Pyrenees\",\n      owner: \"Sara\",\n      size: \"XL\",\n      description: \"A great pyrenees mix often confused with a horse\",\n      image: 'https://vetstreet.brightspotcdn.com/dims4/default/a5cb6bb/2147483647/thumbnail/645x380/quality/90/?url=https%3A%2F%2Fvetstreet-brightspot.s3.amazonaws.com%2F0c%2Ff72de0a8a911e0a0d50050568d634f%2Ffile%2Fpyrenean-sheepdog-4-645mk070111.jpg'\n    }, \n    {\n      id: 6,\n      name: \"Gabby\",\n    